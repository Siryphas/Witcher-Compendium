{
  "name": "Witcher",
  "type": "race",
  "img": "https://assets.forge-vtt.com/652dd4eea9ac2daadb46a415/modules/wtrpg-complete-compendium/packs/images/Race%20Icons/Race%20Witcher%20Icon.png",
  "system": {
    "descriotion": "",
    "perk1": {
      "name": "Enhanced Senses",
      "description": "<p>Due to their heightened senses, witchers take no penalties from areas of dim light and gain an inherent +1 to their Awareness skill, as well as the ability to track things by scent alone.</p>"
    },
    "perk2": {
      "name": "Dulled Emotions",
      "description": "<p>\u0017Thanks to trauma and mutation, a witcher&rsquo;s emotions are dulled. Witchers do not have to make courage checks against Intimidation, but they have a -4 to their Empathy Stat. \u0017 This cannot bring Empathy below 1.</p>"
    },
    "perk3": {
      "name": "Resilient Mutation",
      "description": "<p>A\u0016fter all of the mutations required to become a witcher, they are immune to diseases and are able to use mutagens.</p>"
    },
    "perk4": {
      "name": "Lightning Re\u001dflexes",
      "description": "<p>A\u0016fter intensive training and mutation, witchers are much faster and more agile than humans. They gain a permanent +1 to their Re\u0015flex and Dexterity that can raise both stats above 10.</p>"
    },
    "socialStanding": {
      "north": "hatedFeared",
      "nilfgaard": "hatedFeared",
      "skellige": "tolerated",
      "dolBlathanna": "tolerated",
      "mahakam": "tolerated"
    }
  },
